<Window x:Class="PL_GUI.MainMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL_GUI"
        mc:Ignorable="d"
        Title="SecurityTK/MainMenu" Height="400" Width="400" Background="#FFE5E2D0">
    <Grid>
        <Label Background="White" HorizontalAlignment="Left" Margin="0,0,0,-0.333" Width="393">Please choose an action to proceed</Label>
        <Button x:Name="Data_Leakage" FontSize="20" Click="Data_Leakage_Left_Button_Click" Margin="112,144,108.333,138.667" Content="Data-Leakage" Background="#FFDDDDDD"/>
        <Button x:Name="User_Management" FontSize="20" Click="User_Management_Left_Button_Click" Margin="112,34,108.333,248.667" Content="User Management" Background="#FFDDDDDD"/>
        <Button x:Name="File_Crypto" FontSize="20" Click="File_Crypto_Left_Button_Click" Margin="112,256,108.333,26.667" Content="File Crypto" Background="#FFDDDDDD"/>
    </Grid>
</Window>
