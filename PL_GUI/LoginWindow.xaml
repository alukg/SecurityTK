<Window x:Class="PL_GUI.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL_GUI"
        mc:Ignorable="d"
        Title="SecurityTK/LoginWindow" Height="400" Width="400">
    <Grid>
        <StackPanel Margin="15,20,19.333,14.667">
            <Label>Username:</Label>
            <TextBox x:Name="Username" Margin="0,0,161.333,0" />
            <Label>Password:</Label>
            <PasswordBox Name="Password" MaxLength="8" Margin="0,0,162.333,0" />
        </StackPanel>
        <Button FontSize="18" Click="Left_Enter_Button_Click" Margin="68,131,218,193" Content="Login"/>
        <Button FontSize="18" Click="Left_Refresh_Button_Click" Margin="208,131,78,193" Content="Refresh"/>

    </Grid>
</Window>
