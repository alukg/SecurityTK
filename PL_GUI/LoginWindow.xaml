<Window x:Class="PL_GUI.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL_GUI"
        mc:Ignorable="d"
        Title="LoginWindow" Height="400" Width="400">
    <Grid>
        <StackPanel Margin="15">
            <Label>Username:</Label>
            <TextBox Name="Username" />
            <Label>Password:</Label>
            <PasswordBox Name="Password" MaxLength="8" />
        </StackPanel>
        <Button FontSize="20" Click="Left_Enter_Button_Click" Margin="37,123,221.333,195.667" Content="Enter"/>
        <Button FontSize="20" Click="Left_Refresh_Button_Click" Margin="217,123,41.333,195.667" Content="Refresh"/>

    </Grid>
</Window>
